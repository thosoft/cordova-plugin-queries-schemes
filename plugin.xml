<?xml version='1.0' encoding='UTF-8'?>
<plugin id="cordova-plugin-queries-schemes" version="0.1.1" xmlns="http://apache.org/cordova/ns/plugins/1.0">
    <name>App Queries Strings</name>
    <author>Rob Hilgefort (http://rob.hilgefort.me)</author>
    <description>
      Cordova / PhoneGap Plugin to work arround Apple's iOS9 requirement that URL Schemes
      be declared and submitted to Apple (via Info.plist file)
    </description>
    <keywords>cordova, ios, app, queries, url, schemes, lsapplicationqueriesschemes</keywords>
    <license>MIT</license>
    <engines>
        <engine name="cordova" version=">=3.0.0"/>
    </engines>
    <platform name="ios">
      <config-file target="*-Info.plist" parent="LSApplicationQueriesSchemes">
        <array>
          <string>fbapi</string>
          <string>fbapi20130214</string>
          <string>fbapi20130410</string>
          <string>fbapi20130702</string>
          <string>fbapi20131010</string>
          <string>fbapi20131219</string>
          <string>fbapi20140410</string>
          <string>fbapi20140116</string>
          <string>fbapi20150313</string>
          <string>fbapi20150629</string>
          <string>fbapi20160210</string>
          <string>fbauth</string>
          <string>fbauth2</string>
          <string>fb-messenger-api20140430</string>
        </array>
      </config-file>
    </platform>
</plugin>
